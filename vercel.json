{
    "version": 2,
    "builds": [
        {
            "src": "start.sh",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "start.sh"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PORT": "8080"
    }
} 